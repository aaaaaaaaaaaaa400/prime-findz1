    <style>
      /* Mobile Animation Styles */
      @media (max-width: 767px) {
        /* Animated entrance for benefit cards */
        .benefit-card {
          transform: translateY(30px);
          opacity: 0;
          transition: transform 0.6s ease-out, opacity 0.6s ease-out;
        }
        
        .benefit-card.show {
          transform: translateY(0);
          opacity: 1;
        }
        
        /* Enhanced touch feedback */
        .theme-btn, .service-icon, .benefit-card, .footer-content a, .list-items a {
          transition: transform 0.3s ease, box-shadow 0.3s ease;
        }
        
        /* Floating animation for icons */
        .service-icon, .benefit-icon {
          animation: float 3s ease-in-out infinite;
        }
        
        @keyframes float {
          0% { transform: translateY(0px); }
          50% { transform: translateY(-10px); }
          100% { transform: translateY(0px); }
        }
        
        /* Pulse animation for CTA buttons */
        .hero-button .theme-btn {
          animation: pulse-mobile 2s infinite;
        }
        
        @keyframes pulse-mobile {
          0% { box-shadow: 0 0 0 0 rgba(67, 195, 234, 0.6); }
          70% { box-shadow: 0 0 0 15px rgba(67, 195, 234, 0); }
          100% { box-shadow: 0 0 0 0 rgba(67, 195, 234, 0); }
        }
        
        /* Smooth scrolling for better mobile experience */
        html {
          scroll-behavior: smooth;
        }
      }
      
      /* FAQ Illustration Styles */
      .faq-illustration {
        background-color: rgba(67, 195, 234, 0.05);
        border-radius: 20px;
        padding: 40px;
        height: 100%;
        display: flex;
        flex-direction: column;
        justify-content: center;
        position: relative;
        overflow: hidden;
      }
      
      .faq-illustration::before {
        content: "";
        position: absolute;
        width: 200px;
        height: 200px;
        background: rgba(67, 195, 234, 0.1);
        border-radius: 50%;
        top: -100px;
        left: -100px;
      }
      
      .faq-illustration::after {
        content: "";
        position: absolute;
        width: 150px;
        height: 150px;
        background: rgba(38, 132, 255, 0.1);
        border-radius: 50%;
        bottom: -50px;
        right: -50px;
      }
      
      .house-illustration {
        text-align: center;
        position: relative;
        z-index: 2;
      }
      
      .house-icon {
        font-size: 120px;
        color: #43c3ea;
        margin-bottom: 30px;
        animation: float-icon 3s ease-in-out infinite;
      }
      
      @keyframes float-icon {
        0% { transform: translateY(0px); }
        50% { transform: translateY(-10px); }
        100% { transform: translateY(0px); }
      }
      
      .insulation-benefits {
        text-align: center;
        position: relative;
        z-index: 2;
      }
      
      .insulation-benefits h3 {
        color: #333;
        font-weight: 600;
        margin-bottom: 15px;
      }
      
      .insulation-benefits p {
        color: #666;
      }
      
      .benefit-icon-row {
        display: flex;
        justify-content: center;
        margin: 20px 0;
      }
      
      .benefit-icon {
        width: 60px;
        height: 60px;
        background: #fff;
        border-radius: 50%;
        display: flex;
        align-items: center;
        justify-content: center;
        margin: 0 10px;
        box-shadow: 0 5px 15px rgba(0, 0, 0, 0.08);
      }
      
      .benefit-icon i {
        font-size: 24px;
        color: #43c3ea;
      }
      
      .faq-accordion .accordion-item {
        transition: transform 0.3s ease, box-shadow 0.3s ease;
      }
      
      .faq-accordion .accordion-item:hover {
        transform: translateX(5px);
        box-shadow: 0 5px 15px rgba(0, 0, 0, 0.1);
      }
      
      .accordion-button {
        transition: background-color 0.3s ease;
      }
      
      .accordion-button:not(.collapsed) {
        background: linear-gradient(90deg, rgba(67, 195, 234, 0.1) 0%, rgba(255, 255, 255, 0) 100%);
      }
      
      .accordion-button span {
        display: inline-block;
        transition: transform 0.3s ease;
      }
      
      .accordion-button:hover span {
        transform: translateX(3px);
        color: #43c3ea;
      }
      
      /* Enhanced Counter Section Styles */
      .counter-items {
        position: relative;
        overflow: hidden;
        padding: 35px 20px;
        border-radius: 15px;
        background: #fff;
        box-shadow: 0 10px 30px rgba(0, 0, 0, 0.06);
        transition: all 0.4s ease;
        z-index: 1;
        height: 100%;
        display: flex;
        flex-direction: column;
        align-items: center;
        justify-content: center;
        text-align: center;
        margin: 0 5px;
      }
      
      .counter-section .row {
        margin: 0 -5px;
      }
      
      .counter-section .col-xl {
        padding: 0 5px;
      }
      
      .counter-items::before {
        content: '';
        position: absolute;
        top: 0;
        left: 0;
        width: 3px;
        height: 100%;
        background: #43c3ea;
        transition: all 0.4s ease;
      }
      
      .counter-items:hover {
        transform: translateY(-10px);
        box-shadow: 0 15px 40px rgba(67, 195, 234, 0.15);
      }
      
      .counter-items:hover::before {
        width: 100%;
        opacity: 0.03;
      }
      
      .counter-items .icon {
        position: relative;
        z-index: 2;
        margin-bottom: 15px;
        transition: all 0.4s ease;
      }
      
      .counter-items:hover .icon i {
        animation: pulse-count 1.5s infinite;
        color: #43c3ea;
      }
      
      .counter-items .content {
        position: relative;
        z-index: 2;
      }
      
      .counter-items .content h2 {
        font-size: 42px;
        font-weight: 700;
        margin-bottom: 5px;
        background: linear-gradient(90deg, #43c3ea 0%, #2684ff 100%);
        -webkit-background-clip: text;
        -webkit-text-fill-color: transparent;
      }
      
      .counter-items .content h3 {
        font-size: 18px;
        font-weight: 600;
        color: #333;
        margin: 0;
      }
      
      @keyframes pulse-count {
        0% { transform: scale(1); }
        50% { transform: scale(1.1); }
        100% { transform: scale(1); }
      }
      
      @keyframes rotate-tools {
        0% { transform: rotate(0deg); }
        25% { transform: rotate(15deg); }
        50% { transform: rotate(0deg); }
        75% { transform: rotate(-15deg); }
        100% { transform: rotate(0deg); }
      }
      
      /* Modern Offcanvas Styles */
      .modern-sidebar {
        background: linear-gradient(145deg, #ffffff, #f8f9fa);
        box-shadow: 0 5px 30px rgba(0, 0, 0, 0.1);
        padding: 30px;
        border-radius: 15px;
        width: 100%;
        max-width: 400px;
        transition: all 0.3s ease;
      }
      
      .offcanvas__overlay.active {
        backdrop-filter: blur(5px);
      }
      
      .logo-animation {
        display: inline-block;
        animation: float-subtle 3s ease-in-out infinite;
      }
      
      .close-btn {
        background: transparent;
        border: none;
        font-size: 22px;
        color: #333;
        cursor: pointer;
        transition: all 0.3s ease;
        padding: 8px;
        border-radius: 50%;
      }
      
      .close-btn:hover {
        background-color: rgba(220, 53, 69, 0.1);
        color: #dc3545;
        transform: rotate(90deg);
      }
      
      .pulse-effect {
        transition: all 0.3s ease;
      }
      
      .pulse-effect:hover {
        animation: pulse 1s infinite;
      }
      
      .sidebar-subtitle {
        font-size: 14px;
        color: #6c757d;
        text-transform: uppercase;
        letter-spacing: 1px;
        display: block;
        margin-bottom: 5px;
      }
      
      .sidebar-title {
        font-size: 24px;
        font-weight: 700;
        color: #23c56d;
        margin-bottom: 10px;
      }
      
      .sidebar-separator {
        height: 3px;
        width: 50px;
        background: linear-gradient(90deg, #23c56d, #17a2b8);
        margin-bottom: 15px;
        border-radius: 2px;
      }
      
      .contact-title {
        font-size: 18px;
        font-weight: 600;
        color: #333;
        margin-bottom: 20px;
        display: flex;
        align-items: center;
        border-bottom: 1px solid #eee;
        padding-bottom: 10px;
      }
      
      .contact-title-icon {
        color: #23c56d;
        margin-right: 10px;
      }
      
      .contact-list {
        padding: 0;
        list-style: none;
      }
      
      .contact-item {
        margin-bottom: 15px;
        transition: all 0.3s ease;
      }
      
      .contact-item:hover {
        transform: translateX(5px);
      }
      
      .contact-icon-wrapper {
        width: 35px;
        height: 35px;
        background-color: rgba(35, 197, 109, 0.1);
        border-radius: 50%;
        display: flex;
        align-items: center;
        justify-content: center;
        margin-right: 15px;
        color: #23c56d;
        transition: all 0.3s ease;
      }
      
      .contact-item:hover .contact-icon-wrapper {
        background-color: #23c56d;
        color: white;
      }
      
      .cta-container {
        margin-top: 30px;
      }
      
      .sidebar-cta {
        display: flex;
        align-items: center;
        background: linear-gradient(90deg, #23c56d, #17a2b8);
        color: white;
        padding: 12px 20px;
        border-radius: 50px;
        text-decoration: none;
        font-weight: 600;
        transition: all 0.3s ease;
        box-shadow: 0 5px 15px rgba(35, 197, 109, 0.2);
        position: relative;
        overflow: hidden;
      }
      
      .sidebar-cta:before {
        content: '';
        position: absolute;
        top: 0;
        left: -100%;
        width: 100%;
        height: 100%;
        background: linear-gradient(90deg, transparent, rgba(255, 255, 255, 0.2), transparent);
        transition: 0.5s;
      }
      
      .sidebar-cta:hover:before {
        left: 100%;
      }
      
      .sidebar-cta:hover {
        transform: translateY(-3px);
        box-shadow: 0 8px 20px rgba(35, 197, 109, 0.3);
        color: white;
        text-decoration: none;
      }
      
      .cta-icon-wrap {
        margin-right: 10px;
        font-size: 18px;
      }
      
      .cta-text {
        flex-grow: 1;
      }
      
      .cta-arrow {
        margin-left: 10px;
        transition: transform 0.3s ease;
      }
      
      .sidebar-cta:hover .cta-arrow {
        transform: translateX(5px);
      }
      
      .social-title {
        font-size: 16px;
        font-weight: 600;
        color: #333;
        margin-bottom: 15px;
      }
      
      .social-icons {
        display: flex;
        align-items: center;
        gap: 12px;
      }
      
      .social-icon-item {
        width: 40px;
        height: 40px;
        border-radius: 50%;
        display: flex;
        align-items: center;
        justify-content: center;
        color: white;
        font-size: 16px;
        transition: all 0.3s ease;
        transform: translateY(0);
      }
      
      .social-icon-item:hover {
        transform: translateY(-5px);
        color: white;
        text-decoration: none;
      }
      
      .social-icon-item.facebook {
        background-color: #3b5998;
      }
      
      .social-icon-item.twitter {
        background-color: #1da1f2;
      }
      
      .social-icon-item.youtube {
        background-color: #ff0000;
      }
      
      .social-icon-item.linkedin {
        background-color: #0077b5;
      }
      
      /* Animation Classes */
      .fade-in-element {
        opacity: 0;
        transform: translateY(20px);
      }
      
      .zoom-in-element {
        opacity: 0;
        transform: scale(0.9);
      }
      
      @keyframes fade-in-up {
        to {
          opacity: 1;
          transform: translateY(0);
        }
      }
      
      @keyframes zoom-in {
        to {
          opacity: 1;
          transform: scale(1);
        }
      }
      
      @keyframes rotate {
        0% { transform: rotate(0deg); }
        100% { transform: rotate(0deg); }
      }
      
      @keyframes completion-pulse {
        0% {
          transform: scale(1);
          box-shadow: 0 0 0 0 rgba(0, 128, 0, 0.7);
        }
        
        50% {
          transform: scale(1.05);
          box-shadow: 0 0 0 10px rgba(0, 128, 0, 0);
        }
        
        100% {
          transform: scale(1);
          box-shadow: 0 0 0 0 rgba(0, 128, 0, 0);
        }
      }
      
      @keyframes pulse {
        0% {
          transform: scale(0.95);
          box-shadow: 0 0 0 0 rgba(220, 53, 69, 0.3);
        }
        
        70% {
          transform: scale(1);
          box-shadow: 0 0 0 10px rgba(220, 53, 69, 0);
        }
        
        100% {
          transform: scale(0.95);
          box-shadow: 0 0 0 0 rgba(220, 53, 69, 0);
        }
      }
      
      /* Logo Animation */
      @keyframes float-subtle {
        0% {
          transform: translateY(0px);
        }
        50% {
          transform: translateY(-5px);
        }
        100% {
          transform: translateY(0px);
        }
      }
      
      /* Enhanced Animations */
      @keyframes slide-up {
        from {
          opacity: 0;
          transform: translateY(30px);
        }
        to {
          opacity: 1;
          transform: translateY(0);
        }
      }
      
      @keyframes slide-right {
        from {
          opacity: 0;
          transform: translateX(-30px);
        }
        to {
          opacity: 1;
          transform: translateX(0);
        }
      }
      
      @keyframes fade-in {
        from {
          opacity: 0;
        }
        to {
          opacity: 1;
        }
      }
      
      @keyframes scale-in {
        from {
          opacity: 0;
          transform: scale(0.8);
        }
        to {
          opacity: 1;
          transform: scale(1);
        }
      }
      
      /* Animated Hamburger Icon */
      .animated-icon {
        position: relative;
        transition: all 0.3s ease;
        overflow: hidden;
      }
      
      .animated-icon img {
        transition: all 0.4s ease;
      }
      
      .animated-icon::before {
        content: '';
        position: absolute;
        width: 100%;
        height: 100%;
        background: rgba(67, 195, 234, 0.1);
        border-radius: 50%;
        transform: scale(0);
        transition: all 0.3s ease;
      }
      
      .animated-icon:hover::before {
        transform: scale(1.5);
      }
      
      .animated-icon:hover img {
        transform: rotate(90deg);
      }
      
      /* Mobile-only styles */
      @media (max-width: 767px) {
        /* Mobile navigation styles */
        .mobile-nav-indicator {
          display: block;
          font-size: 12px;
          color: #fff;
          background-color: rgba(67, 195, 234, 0.9);
          padding: 5px 10px;
          border-radius: 4px;
          margin-bottom: 15px;
          text-align: center;
          animation: slide-right 0.5s ease forwards;
        }
        
        /* Hide on desktop */
        .desktop-hide {
          display: none;
        }
        
        /* Show only on mobile */
        .mobile-only {
          display: block;
        }
      }
      
      /* Mobile-only styles */
      @media (max-width: 767px) {
        /* Mobile navigation styles */
        .mobile-breadcrumb {
          position: fixed;
          top: 70px;
          left: 0;
          right: 0;
          z-index: 99;
          padding: 0 15px;
          transition: all 0.3s ease;
        }
        
        .mobile-nav-indicator {
          display: block;
          font-size: 13px;
          color: #fff;
          background: linear-gradient(135deg, #43c3ea 0%, #2684ff 100%);
          padding: 8px 15px;
          border-radius: 50px;
          margin-bottom: 15px;
          text-align: center;
          box-shadow: 0 3px 10px rgba(67, 195, 234, 0.3);
          animation: slide-right 0.5s ease forwards;
          transform: translateX(0);
          opacity: 0.9;
          backdrop-filter: blur(5px);
        }
        
        .mobile-nav-indicator i {
          margin-right: 5px;
          animation: pulse 1.5s infinite;
          display: inline-block;
        }
        
        /* Scroll progress indicator */
        .scroll-progress {
          position: fixed;
          top: 0;
          left: 0;
          width: 0%;
          height: 3px;
          background: linear-gradient(to right, #43c3ea, #2684ff);
          z-index: 9999;
          transition: width 0.2s ease;
        }
        
        /* Enhance mobile animations */
        .header-top-section {
          display: none; /* Simplify mobile header */
        }
        
        /* Hide on desktop */
        .desktop-hide {
          display: none !important;
        }
        
        /* Show only on mobile */
        .mobile-only {
          display: block !important;
        }
      }
      
      /* Animate sections as they come into view */
      .animate-on-scroll {
        opacity: 0;
        transition: all 0.6s ease;
      }
      
      .animate-on-scroll.slide-up {
        transform: translateY(30px);
      }
      
      .animate-on-scroll.slide-right {
        transform: translateX(-30px);
      }
      
      .animate-on-scroll.scale-in {
        transform: scale(0.9);
      }
      
      .animate-on-scroll.visible {
        opacity: 1;
        transform: translate(0) scale(1);
      }
    </style>
  </head>
