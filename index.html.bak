  <script>
    // Counter Animation and Smooth Scrolling
    document.addEventListener('DOMContentLoaded', function() {
      // Counter animation implementation
      function initCounters() {
        const counterElements = document.querySelectorAll('.count');
        const counterObserver = new IntersectionObserver((entries) => {
          entries.forEach(entry => {
            if (entry.isIntersecting) {
              const target = entry.target;
              const targetValue = parseInt(target.textContent);
              let count = 0;
              const duration = 2000; // ms
              const increment = Math.ceil(targetValue / (duration / 16)); // 60fps approx
              
              // Stop any existing animation
              if (target.countInterval) clearInterval(target.countInterval);
              
              // Start a new animation
              target.countInterval = setInterval(() => {
                count += increment;
                if (count >= targetValue) {
                  target.textContent = targetValue;
                  clearInterval(target.countInterval);
                } else {
                  target.textContent = count;
                }
              }, 16);
              
              // Unobserve after triggering animation
              counterObserver.unobserve(target);
            }
          });
        }, { threshold: 0.5 });
        
        counterElements.forEach(counter => {
          counterObserver.observe(counter);
        });
      }
      
      // Initialize counters
      initCounters();
      
      // Smooth scrolling for navigation
      const scrollLinks = document.querySelectorAll('[data-scroll-to]');
      
      scrollLinks.forEach(link => {
        link.addEventListener('click', function(e) {
          e.preventDefault();
          
          // Get the target section
          const targetId = this.getAttribute('data-scroll-to');
          const targetElement = document.getElementById(targetId);
          
          if (targetElement) {
            // Calculate header height for offset
            const headerHeight = document.querySelector('.header-section-2').offsetHeight;
            
            // Calculate the target position with offset
            const targetPosition = targetElement.getBoundingClientRect().top + window.pageYOffset - headerHeight;
            
            // Smooth scroll to target
            window.scrollTo({
              top: targetPosition,
              behavior: 'smooth'
            });
            
            // Close mobile menu if open
            const offcanvasOverlay = document.querySelector('.offcanvas__overlay');
            if (offcanvasOverlay.classList.contains('active')) {
              const closeBtn = document.querySelector('.offcanvas__close button');
              if (closeBtn) closeBtn.click();
            }
            
            // Update mobile breadcrumb if on mobile
            if (window.innerWidth <= 767) {
              const currentPageElement = document.getElementById('current-page');
              if (currentPageElement) {
                const sectionName = targetId.charAt(0).toUpperCase() + targetId.slice(1);
                currentPageElement.textContent = sectionName;
              }
            }
          }
        });
      });
      
      // Fix navigation for hero section (no id)
      const homeLink = document.querySelector('[data-scroll-to="hero-section"]');
      if (homeLink) {
        homeLink.addEventListener('click', function(e) {
          e.preventDefault();
          
          // Scroll to top
          window.scrollTo({
            top: 0,
            behavior: 'smooth'
          });
          
          // Close mobile menu if open
          const offcanvasOverlay = document.querySelector('.offcanvas__overlay');
          if (offcanvasOverlay.classList.contains('active')) {
            const closeBtn = document.querySelector('.offcanvas__close button');
            if (closeBtn) closeBtn.click();
          }
          
          // Update mobile breadcrumb
          if (window.innerWidth <= 767) {
            const currentPageElement = document.getElementById('current-page');
            if (currentPageElement) {
              currentPageElement.textContent = 'Home';
            }
          }
        });
      }
    });
  </script>
