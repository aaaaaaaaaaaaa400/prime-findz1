              'transform': 'translateY(20px)'
            }).animate({
              'opacity': '1',
              'transform': 'translateY(0)'
            }, 500);
            
            // Staggered animation for contact items
            $('.contact-item').each(function(index) {
              var $this = $(this);
              setTimeout(function() {
                $this.css({
                  'opacity': '0',
                  'transform': 'translateX(-20px)'
                }).animate({
                  'opacity': '1',
                  'transform': 'translateX(0)'
                }, 400);
              }, 300 + (index * 150));
            });
            
            // Animate CTA and social icons
            setTimeout(function() {
              $('.cta-container, .social-media-container').css({
                'opacity': '0',
                'transform': 'translateY(20px)'
              }).animate({
                'opacity': '1',
                'transform': 'translateY(0)'
              }, 500);
            }, 800);
          }, 300);
        });
        
        // Enhance close animation
        $('.offcanvas__close button').on('click', function() {
          $('.sidebar-intro, .contact-item, .cta-container, .social-media-container').animate({
            'opacity': '0',
            'transform': 'translateY(20px)'
          }, 300);
        });
        
        // Enhance hover effects on contact items
        $('.contact-item').hover(
          function() {
            $(this).find('.contact-icon-wrapper').css('transform', 'scale(1.1) rotate(10deg)');
          },
          function() {
            $(this).find('.contact-icon-wrapper').css('transform', 'scale(1) rotate(0deg)');
          }
        );
        
        // Add scroll animation to elements
        function handleScrollAnimation() {
          $('.animate-on-scroll').each(function() {
            var elementPosition = $(this).offset().top;
            var topOfWindow = $(window).scrollTop();
            var windowHeight = $(window).height();
            
            if (elementPosition < topOfWindow + windowHeight - 100) {
              $(this).addClass('visible');
            }
          });
        }
        
        // Add animation classes to sections
        $('.section-title, .about-content, .benefit-card, .work-process-items, .faq-content, .counter-items').addClass('animate-on-scroll slide-up');
        $('.about-image, .house-illustration').addClass('animate-on-scroll scale-in');
        $('.hero-content, .about-list li').addClass('animate-on-scroll slide-right');
        
        // Execute on page load and scroll
        handleScrollAnimation();
        $(window).on('scroll', handleScrollAnimation);
        
        // Mobile breadcrumb navigation update
        function updateMobileBreadcrumb() {
          var currentSection = '';
          var scrollPosition = $(window).scrollTop();
          
          // Determine current section based on scroll position
          if (scrollPosition < $('#about').offset().top - 100) {
            currentSection = 'Home';
          } else if (scrollPosition < $('#service').offset().top - 100) {
            currentSection = 'About Us';
          } else if (scrollPosition < $('#faq').offset().top - 100) {
            currentSection = 'Services';
          } else if (scrollPosition < $('.counter-section').offset().top - 100) {
            currentSection = 'FAQ';
          } else {
            currentSection = 'Contact';
          }
          
          // Update the breadcrumb text with animation
          if ($('#current-page').text() !== currentSection) {
            $('#current-page').fadeOut(200, function() {
              $(this).text(currentSection).fadeIn(200);
            });
          }
        }
        
        // Update breadcrumb on scroll
        $(window).on('scroll', updateMobileBreadcrumb);
        updateMobileBreadcrumb();
      });
    </script>
    
